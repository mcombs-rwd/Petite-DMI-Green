                {
                  "latitude": "33.73504",
                  "longitude": "-84.37096",
                  "title": "Grant Park",
                  "layoutTemplateID": "imageAboveText",
                  "regions": [
                    {
                      "type": "media",
                      "regionID": "1",
                      "assetType": "image",
                      "assetMedia": {
                        "full": {
                          "url": "/assets/grantPark/0/grantPark.jpg"
                        },
